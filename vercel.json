{
  "version": 2,
  "name": "dig345-p2-emotionai",
  "builds": [{
      "src": "./index.py",
      "use": "@vercel/python"
  }],
  "routes": [{
      "src": "/(.*)",
      "dest": "/index.py"
  }],
  "env": {
    "FLASK_ENV": "production"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index" }
  ]
}