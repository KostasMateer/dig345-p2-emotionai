{
    "version": 2,
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python"
      },
      {
        "use": "@vercel/static",
        "src": "build",
        "config": {}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "build": {
      "env": {
        "FLASK_ENV": "production"
      },
      "command": "pip install -r requirements.txt && echo 'done'"
    }
  }
  